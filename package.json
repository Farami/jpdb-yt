{
  "name": "jpdb-yt",
  "version": "0.0.1",
  "description": "a youtube integration for jpdb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonghakseo/chrome-extension-boilerplate-react-vite.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:watch": "set __DEV__=true && vite build --watch",
    "build:hmr": "rollup --config utils/reload/rollup.config.ts",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "npm run build:hmr && (run-p wss build:watch)",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "axios": "^1.3.3",
    "he": "^1.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "striptags": "^3.2.0",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@testing-library/react": "13.4.0",
    "@types/chrome": "0.0.197",
    "@types/he": "^1.2.0",
    "@types/jest": "29.0.3",
    "@types/node": "18.7.23",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "@vitejs/plugin-react": "2.1.0",
    "chokidar": "^3.5.3",
    "fs-extra": "10.1.0",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "npm-run-all": "^4.1.5",
    "rollup": "2.79.1",
    "rome": "^11.0.0",
    "sass": "1.55.0",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.1",
    "typescript": "4.8.3",
    "vite": "3.1.3",
    "ws": "8.9.0"
  }
}
